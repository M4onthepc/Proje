    ViewBag.Title = "Hakkımızda";
    Layout = "~/Views/Shared/_SiteLayout.cshtml";
}

<section class="about" data-aos="fade-up">
    <div class="container"><h1>HAKKIMIZDA</h1>
        <div class="row">
